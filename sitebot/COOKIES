Quickreference of cookies:
        (ANNOUNCES)       
##########################
## Read %u_ as 'User'
## Read %g_ as 'Group'
## Read %t_ as 'Total'

# BADBITRATE:
%bitrate, %g_name, %u_name

# BADGENRE:
%g_name, %genre, %u_name

# BADYEAR:
%g_name, %u_name, %year

# COMPLETE:
%filename, %g_name, %releasename, %t_files, %t_length, %t_mbytes, %u_avgspeed, %u_name

# COMPLETE_AUDIO:
%filename, %g_name, %releasename, %t_files, %t_length, %t_mbytes, %u_avgspeed, %u_name

# COMPLETE_OTHER:
%filename, %g_name, %releasename, %t_files, %t_length, %t_mbytes, %u_avgspeed, %u_name

# COMPLETE_RACE:
%filename, %g_avgspeed_winner, %g_count, %g_files_winner, %g_mbytes_winner, %g_name
%g_name_fastest, %g_name_slowest, %g_name_winner, %g_percent_winner, %releasename
%t_files, %t_length, %t_mbytes, %u_avgspeed, %u_avgspeed_fastest, %u_avgspeed_slowest
%u_avgspeed_winner, %u_count, %u_files_winner, %u_mbytes_winner, %u_name_fastest
%u_name_slowest, %u_name_winner, %u_percent_winner

# COMPLETE_RACE_AUDIO:
%filename, %g_avgspeed_winner, %g_count, %g_files_winner, %g_mbytes_winner, %g_name
%g_name_fastest, %g_name_slowest, %g_name_winner, %g_percent_winner, %releasename
%t_files, %t_length, %t_mbytes, %u_avgspeed, %u_avgspeed_fastest, %u_avgspeed_slowest
%u_avgspeed_winner, %u_count, %u_files_winner, %u_mbytes_winner, %u_name_fastest
%u_name_slowest, %u_name_winner, %u_percent_winner

# COMPLETE_RACE_OTHER:
%filename, %g_avgspeed_winner, %g_count, %g_files_winner, %g_mbytes_winner, %g_name
%g_name_fastest, %g_name_slowest, %g_name_winner, %g_percent_winner, %releasename
%t_files, %t_length, %t_mbytes, %u_avgspeed, %u_avgspeed_fastest, %u_avgspeed_slowest
%u_avgspeed_winner, %u_count, %u_files_winner, %u_mbytes_winner, %u_name_fastest
%u_name_slowest, %u_name_winner, %u_percent_winner

# COMPLETE_RACE_RAR:
%filename, %g_avgspeed_winner, %g_count, %g_files_winner, %g_mbytes_winner, %g_name
%g_name_fastest, %g_name_slowest, %g_name_winner, %g_percent_winner, %releasename
%t_files, %t_length, %t_mbytes, %u_avgspeed, %u_avgspeed_fastest, %u_avgspeed_slowest
%u_avgspeed_winner, %u_count, %u_files_winner, %u_mbytes_winner, %u_name_fastest
%u_name_slowest, %u_name_winner, %u_percent_winner

# COMPLETE_RACE_VIDEO:
%filename, %g_avgspeed_winner, %g_count, %g_files_winner, %g_mbytes_winner, %g_name
%g_name_fastest, %g_name_slowest, %g_name_winner, %g_percent_winner, %releasename
%t_files, %t_length, %t_mbytes, %u_avgspeed, %u_avgspeed_fastest, %u_avgspeed_slowest
%u_avgspeed_winner, %u_count, %u_files_winner, %u_mbytes_winner, %u_name_fastest
%u_name_slowest, %u_name_winner, %u_percent_winner

# COMPLETE_RACE_ZIP:
%filename, %g_avgspeed_winner, %g_count, %g_files_winner, %g_mbytes_winner, %g_name
%g_name_fastest, %g_name_slowest, %g_name_winner, %g_percent_winner, %releasename
%t_files, %t_length, %t_mbytes, %u_avgspeed, %u_avgspeed_fastest, %u_avgspeed_slowest
%u_avgspeed_winner, %u_count, %u_files_winner, %u_mbytes_winner, %u_name_fastest
%u_name_slowest, %u_name_winner, %u_percent_winner

# COMPLETE_RAR:
%filename, %g_name, %releasename, %t_files, %t_length, %t_mbytes, %u_avgspeed, %u_name

# COMPLETE_VIDEO:
%filename, %g_name, %releasename, %t_files, %t_length, %t_mbytes, %u_avgspeed, %u_name

# COMPLETE_ZIP:
%filename, %g_name, %releasename, %t_files, %t_length, %t_mbytes, %u_avgspeed, %u_name

# DEBUG:
%msg, %u_pid

# DEFAULT:
%msg

# DELDIR:
%g_name, %u_name, %u_tagline

# DOUBLESFV:
%filename, %g_name, %releasename, %u_name

# HALFWAY_NORACE:
%filename, %g_count, %g_files, %g_mbytes, %g_name, %g_name, %g_percent, %g_speed
%releasename, %t_missingfiles, %u_count, %u_files, %u_mbytes, %u_name, %u_percent
%u_speed

# HALFWAY_RACE:
%filename, %g_count, %g_files, %g_mbytes, %g_name, %g_name, %g_percent, %g_speed
%releasename, %t_missingfiles, %u_count, %u_files, %u_mbytes, %u_name, %u_percent
%u_speed

# IMDB:
%msg

# IMDBFIND:
%msg

# IMDBVAR:
%imdbbusinessdata, %imdbcasting, %imdbcastleadchar, %imdbcastleadname, %imdbcertification
%imdbcommentshort, %imdbcountry, %imdbdestination, %imdbdirector, %imdbdirname, %imdbgenre
%imdbislimited, %imdblanguage, %imdblimitedinfo, %imdbname, %imdbnumscreens, %imdbplot
%imdbpremiereinfo, %imdbrating, %imdbruntime, %imdbscore, %imdbtagline, %imdbtitle
%imdburl, %imdbvotes, %imdbyear, {[%imdbbar]}

# INCOMPLETE:
%g_name, %releasename, %u_name

# INVITE:
%g_name, %u_ircnick, %u_name

# LOGIN:
%dummy, %g_name, %u_hostmask, %u_ip, %u_name, %u_pid, %u_tagline

# LOGOUT:
%g_name, %u_hostmask, %u_ip, %u_name, %u_tagline

# NEWDIR:
%g_name, %u_name, %u_tagline

# NEWLEADER:
%filename, %g_count, %g_name, %releasename, %t_length, %t_missingfiles, %u_count
%u_files, %u_mbytes, %u_name, %u_percent, %u_speed

# PRE:
%cds, %files, %g_name, %mbytes, %pregroup, %releasename, %section, %tagline, %user
%who

# RACE:
%against, %filename, %g_count, %g_name, %missing_files, %r_length, %r_percent, %releasename
%t_files, %t_upfiles, %u_count, %u_name, %u_speed

# REQFILLED:
%g_name, %release, %u_name, %u_name_requester, %u_tagline

# REQUEST:
%g_name, %release, %u_name, %u_tagline

# SFV:
%g_name, %releasename, %t_filecount, %u_name

# STAT_GROUPS:
%g_files, %g_mbytes, %g_name, %g_percent, %g_position, %u_avgspeed

# STAT_GROUPS_HEAD:
%dummy

# STAT_POST:
%g_count, %t_files, %t_length, %t_mbytes, %u_avgspeed, %u_count

# STAT_USERS:
%g_name, %u_allup, %u_avgspeed, %u_dayup, %u_files, %u_mbytes, %u_monthup, %u_name
%u_percent, %u_position, %u_wkup

# STAT_USERS_HEAD:
%dummy

# UPDATE:
%filename, %g_name, %releasename, %t_files, %t_mbytes, %u_name, %u_speed

# UPDATE_CBR:
%audio, %bitrate, %filename, %g_name, %genre, %mode, %releasename, %sampling, %t_files
%u_name, %u_speed, %year

# UPDATE_OTHER:
%filename, %g_name, %releasename, %t_files, %t_mbytes, %u_name, %u_speed

# UPDATE_RAR:
%filename, %g_name, %releasename, %t_files, %t_mbytes, %u_name, %u_speed

# UPDATE_VBR:
%a_stream, %audio, %bitrate, %filename, %g_name, %genre, %mode, %releasename, %sampling
%t_files, %u_name, %u_speed, %year

# UPDATE_VIDEO:
%filename, %g_name, %releasename, %t_files, %t_mbytes, %u_name, %u_speed

# UPDATE_ZIP:
%filename, %g_name, %releasename, %t_files, %t_mbytes, %u_name, %u_speed

# WIPE:
%g_name, %u_name, %u_tagline

# WIPE-r:
%g_name, %u_name, %u_tagline

