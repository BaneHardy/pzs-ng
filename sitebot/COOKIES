%releasename, %u_name, %g_name, %filename

# BAD_FILE_GENRE:
%releasename, %u_name, %g_name, %filename

# BAD_FILE_NOSFV:
%releasename, %u_name, %g_name, %filename

# BAD_FILE_SFV:
%releasename, %u_name, %g_name, %filename

# BAD_FILE_WRONGDIR:
%releasename, %u_name, %g_name, %filename

# BAD_FILE_YEAR:
%releasename, %u_name, %g_name, %filename

# BAD_FILE_ZIP:
%releasename, %u_name, %g_name, %filename

# BAD_FILE_ZIPNFO:
%releasename, %u_name, %g_name, %filename

# BANNEDHOST:
%u_name, %u_hostmask, %u_ip

