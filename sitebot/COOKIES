%a_gname %releasename, %a_name, %filename, %g_count, %g_name, %missing_files, %r_gname %t_timeleft
%r_name, %t_duration, %t_files, %t_percent, %t_upfiles, %u_count, %u_name, %u_speed
%u_tagline

# RACE_ZIP:
%a_gname %releasename, %a_name, %filename, %g_count, %g_name, %missing_files, %r_gname %t_timeleft
%r_name, %t_duration, %t_files, %t_percent, %t_upfiles, %u_count, %u_name, %u_speed
%u_tagline

# READDED:
%u_name, %u_siteop

# REQFILLED:
%g_name, %releasename, %u_name, %u_requester, %u_tagline

# REQUEST:
%g_name, %releasename, %u_name, %u_tagline

# SFV_AUDIO:
%g_name, %releasename, %t_filecount, %u_name, %u_tagline

# SFV_OTHER:
%g_name, %releasename, %t_filecount, %u_name, %u_tagline

# SFV_RAR:
%g_name, %releasename, %t_filecount, %u_name, %u_tagline

# SFV_VIDEO:
%g_name, %releasename, %t_filecount, %u_name, %u_tagline

# SYSOP:
%msg

# TAGLINE:
%g_name, %u_name, %u_tagline

# TIMEOUT:
%dummy1, %dummy2, %dummy3, %dummy4, %dummy5, %dummy6, %u_hostmask, %u_idletime, %u_name

# TURGEN:
%msg

# UPDATE_CBR:
%album, %artist, %audio, %bitrate, %codec, %filename, %g_name, %genre, %layer, %mode
%releasename, %sampling, %t_files, %t_mbytes, %t_timeleft, %title, %u_name, %u_speed
%u_tagline, %year

