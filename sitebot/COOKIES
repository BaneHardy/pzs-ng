%filename, %g_name, %releasename, %t_files, %t_mbytes, %t_timeleft, %u_name, %u_speed
%u_tagline

# UPDATE_RAR:
%compression, %filename, %g_name, %releasename, %t_files, %t_mbytes, %t_timeleft
%u_name, %u_speed, %u_tagline

# UPDATE_VBR:
%a_stream, %a_version, %album, %artist, %audio, %bitrate, %codec, %filename, %g_name
%genre, %layer, %mode, %releasename, %sampling, %t_files, %t_mbytes, %t_timeleft
%title, %u_name, %u_speed, %u_tagline, %year

# UPDATE_VIDEO:
%filename, %g_name, %releasename, %t_files, %t_mbytes, %t_timeleft, %u_name, %u_speed
%u_tagline

# UPDATE_ZIP:
%filename, %g_name, %releasename, %t_files, %t_mbytes, %t_timeleft, %u_name, %u_speed
%u_tagline

# WIPE:
%g_name, %u_name, %u_tagline

