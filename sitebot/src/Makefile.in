CC = gcc
RM = rm -f
MAKE = make

CCFLAGS = -W -Wall -lcrypt -lcrypto @STATIC@ -O2 -I../../
CFFLAGS = -W -Wall @STATIC@ -O2

all:	clean bin

bin:	
		${CC} ${CCFLAGS} -o passchk passchk.c /usr/lib/libcrypt.a /usr/lib/libcrypto.a
		${CC} ${CFFLAGS} -o ng-bandwidth ng-bw.c

distclean: clean
	@$(RM) Makefile

clean:	
	@$(RM) ng-bandwidth passchk
